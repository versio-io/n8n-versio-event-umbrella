{
  "name": "@versio-io/n8n-nodes-versio-event",
  "version": "1.0.5",
  "description": "n8n node for sending event data to the Versio.io event processing API",
  "license": "MIT",
  "homepage": "https://live.versio.io/doc/n8n-node/",
  "keywords": [
    "Versio.io Event Umbrella",
    "Versio.io Event",
    "Event",
    "Versio",
    "Versioio",
    "Versio.io",
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Versio.io",
    "email": "hello@versio.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/versio-io/n8n-versio-event-umbrella"
  },
  "scripts": {
    "build": "n8n-node build && rmdir /s /q dist\\documentation",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease",
    "test": "n8n-node lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/VersioEventApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/VersioioEvent/VersioioEvent.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
